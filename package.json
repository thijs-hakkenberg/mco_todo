{
  "name": "our_todo",
  "version": "1.0.0",
  "description": "",
  "main": "dist/index.js",
  "bin": {
    "git-todo-mcp": "./dist/index.js"
  },
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:unit": "jest tests/unit",
    "test:integration": "jest tests/integration",
    "test:e2e": "jest tests/e2e",
    "test:api": "jest src/api/__tests__",
    "build": "tsc",
    "dev": "concurrently \"npm run dev:mcp\" \"npm run dev:api\"",
    "dev:api": "tsx watch src/api/server.ts",
    "dev:mcp": "tsx watch src/index.ts",
    "start": "node dist/index.js",
    "start:api": "node dist/api/server.js",
    "lint": "eslint src tests --ext .ts",
    "format": "prettier --write 'src/**/*.ts' 'tests/**/*.ts'"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "commonjs",
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.20.2",
    "date-fns": "^4.1.0",
    "simple-git": "^3.28.0",
    "uuid": "^13.0.0",
    "uuidv7": "^1.0.2",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.28.5",
    "@babel/preset-typescript": "^7.28.5",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.5",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.9.1",
    "@types/supertest": "^6.0.3",
    "@types/uuid": "^10.0.0",
    "babel-jest": "^30.2.0",
    "concurrently": "^9.2.1",
    "cors": "^2.8.5",
    "express": "^5.1.0",
    "jest": "^30.2.0",
    "socket.io": "^4.8.1",
    "supertest": "^7.1.4",
    "ts-jest": "^29.4.5",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3"
  }
}
